<!DOCTYPE html>
<html lang="en">
    <head>
        <title></title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="/css/styles.css" rel="stylesheet" onload="this.rel = 'stylesheet'" as="stylesheet">
        <link onload="this.rel='stylesheet'" as="stylesheet" rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css" integrity="sha512-Kc323vGBEqzTmouAECnVceyQqyqdsSiqLQISBL29aUW4U/M7pSPA/gEUZQqv1cwx4OnYxTxve5UMg5GT6L4JJg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    </head>
    <body>
        <div class="container bg-neutral-900">
            <div class="analytics h-[100vh] flex justify-end items-start">
                <%- include('partials/global/sidebar') %>
                <div class="main-wrapper min-w-[97%] transition-all">
                    <%- include('partials/global/nav/nav-main') %>
                    <div class="analytics-main h-[80vh] mt-12 px-12 flex justify-between items-start">
                        <div class="analytics-widgets w-1/2">
                            <div class="analytics-count flex justify-between items-center">
                                <%- include('partials/analytics/count',{ icon:"fa-solid fa-globe fa-5x",count:tasks,name:'All' }) %>
                                <%- include('partials/analytics/count',{ icon:"fa-solid fa-spinner fa-5x",count:active,name:"Pending" }) %>
                                <%- include('partials/analytics/count',{ icon:"fa-solid fa-clipboard-list fa-5x",count:completed,name:"Completed" }) %>
                            </div>
                            <%- include('partials/analytics/widget') %>
                        </div>
                        <div class="analytics-chart text-black w-1/2"></div>
                    </div>
                    <%- include('partials/global/footer') %>
                </div>
            </div>
        </div>
        <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
        <script src="/js/app.js"></script>
    </body>
</html>